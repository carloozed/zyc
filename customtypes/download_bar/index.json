{
  "format": "custom",
  "id": "download_bar",
  "label": "Download Bar",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "is_download_available": {
        "type": "Select",
        "config": {
          "label": "Is Download Available",
          "placeholder": "",
          "options": ["Stundenplan", "Resultate", "Nichts"]
        }
      },
      "schedule_is_available": {
        "type": "Text",
        "config": {
          "label": "Schedule is Available",
          "placeholder": "Der Stundenplan ist jetzt verfügbar!"
        }
      },
      "results_are_available": {
        "type": "Text",
        "config": {
          "label": "Results are available",
          "placeholder": "Die Resultate sind jetzt verfügbar!"
        }
      },
      "results_link": {
        "type": "Link",
        "config": {
          "label": "Results Link",
          "placeholder": "Download",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      },
      "schedule_link": {
        "type": "Link",
        "config": {
          "label": "Schedule Link",
          "placeholder": "Download",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      },
      "is_downloadbar_visible": {
        "type": "Boolean",
        "config": {
          "label": "Is Downloadbar VIsible",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      }
    }
  }
}
